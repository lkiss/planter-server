    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.17.3",
    "mongodb": "^3.4.0",
    "mongoose": "^5.8.1",
    "morgan": "~1.9.1",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "concurrently": "^5.0.1",
    "nodemon": "^2.0.2"
  }
